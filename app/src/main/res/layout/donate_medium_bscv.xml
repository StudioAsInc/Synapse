<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<androidx.cardview.widget.CardView
		android:id="@+id/roundLayoutBlockBS"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:cardBackgroundColor="#FFFFFF"
		app:cardElevation="0dp"
		app:cardCornerRadius="20dp">
		<LinearLayout
			android:id="@+id/main"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:paddingLeft="15dp"
			android:paddingTop="10dp"
			android:paddingRight="15dp"
			android:paddingBottom="10dp"
			android:background="@android:color/transparent"
			android:gravity="center_horizontal"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/bs_bar"
				android:layout_width="50dp"
				android:layout_height="5dp"
				android:padding="8dp"
				android:orientation="horizontal"
				android:background="@drawable/bsbar" />
			<TextView
				android:id="@+id/textview48"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="15dp"
				android:layout_marginBottom="25dp"
				android:text="We accept"
				android:textSize="20sp"
				android:textStyle="bold"
				android:textColor="#000000" />
			<LinearLayout
				android:id="@+id/app_holder"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/row1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/crypto"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:layout_marginRight="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview17"
							android:focusable="false"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:padding="8dp"
							android:src="@drawable/bitcoin"
							android:scaleType="fitCenter"
							android:translationX="2.0dp" />
						<TextView
							android:id="@+id/textview26"
							android:focusable="false"
							android:layout_width="80dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal"
							android:text="Crypto"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/binance"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview18"
							android:focusable="false"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:src="@drawable/pays1_2"
							android:scaleType="centerCrop" />
						<TextView
							android:id="@+id/textview27"
							android:focusable="false"
							android:layout_width="80dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="Binance"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/bybit"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:layout_marginLeft="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview19"
							android:focusable="false"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:src="@drawable/bybit"
							android:scaleType="fitCenter"
							android:translationX="5.0dp" />
						<TextView
							android:id="@+id/textview28"
							android:focusable="false"
							android:layout_width="80dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="Bybit"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear50"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="35dp"
					android:layout_marginBottom="35dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/rocket"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:layout_marginRight="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview31"
							android:focusable="false"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:src="@drawable/rocket"
							android:scaleType="fitCenter" />
						<TextView
							android:id="@+id/textview40"
							android:focusable="false"
							android:layout_width="80dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="Rocket"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/bkash"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview32"
							android:focusable="false"
							android:layout_width="100dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:src="@drawable/bkash_1"
							android:scaleType="centerCrop" />
						<TextView
							android:id="@+id/textview41"
							android:focusable="false"
							android:layout_width="80dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="bKash"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/nagad"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:layout_marginLeft="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview33"
							android:focusable="false"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:src="@drawable/nagad"
							android:scaleType="centerCrop" />
						<TextView
							android:id="@+id/textview42"
							android:focusable="false"
							android:layout_width="80dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="Nagad"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear55"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/visa"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:layout_marginRight="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview35"
							android:focusable="false"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:src="@drawable/cards_1"
							android:scaleType="fitCenter" />
						<TextView
							android:id="@+id/textview44"
							android:focusable="false"
							android:layout_width="80dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="VISA Card"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/bank"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview36"
							android:focusable="false"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:padding="2dp"
							android:src="@drawable/bank"
							android:scaleType="fitCenter" />
						<TextView
							android:id="@+id/textview45"
							android:focusable="false"
							android:layout_width="80dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="Bank"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/mastercard"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<ImageView
							android:id="@+id/imageview37"
							android:focusable="false"
							android:layout_width="60dp"
							android:layout_height="60dp"
							android:layout_marginLeft="10dp"
							android:layout_marginRight="12dp"
							android:src="@drawable/cards_2"
							android:scaleType="fitCenter"
							android:translationX="15.0dp" />
						<TextView
							android:id="@+id/textview46"
							android:focusable="false"
							android:layout_width="95dp"
							android:layout_height="wrap_content"
							android:layout_marginTop="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="Mastercard"
							android:textSize="14sp"
							android:textColor="#000000"
							android:translationX="15.0dp" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<TextView
				android:id="@+id/links"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="30dp"
				android:paddingLeft="15dp"
				android:paddingRight="15dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="Please install the app from the verified source. Unauthorized personnel can manipulate payment details"
				android:textSize="10sp"
				android:textColor="#9E9E9E"
				android:clickable="true"
				android:focusable="true" />
		</LinearLayout>
	</androidx.cardview.widget.CardView>
</LinearLayout>
