<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<androidx.swiperefreshlayout.widget.SwipeRefreshLayout
		android:id="@+id/swipe_refresh_layout"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF">
		<ScrollView
			android:id="@+id/scroll"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:scrollbars="none">
			<LinearLayout
				android:id="@+id/scroll_body"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/app_user_info"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:layout_marginTop="16dp"
					android:layout_marginRight="16dp"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<androidx.cardview.widget.CardView
						android:id="@+id/app_user_info_profile_card"
						android:layout_width="48dp"
						android:layout_height="48dp"
						app:cardElevation="0dp">
						<ImageView
							android:id="@+id/app_user_info_profile_image"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:src="@drawable/avatar"
							android:scaleType="centerCrop" />
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/app_user_info_username"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="16dp"
						android:text="TextView"
						android:textSize="20sp"
						android:textStyle="bold"
						android:textColor="#000000" />
					<ImageView
						android:id="@+id/app_user_info_gender"
						android:layout_width="18dp"
						android:layout_height="18dp"
						android:layout_marginLeft="8dp"
						android:src="@drawable/male_badge"
						android:scaleType="centerInside" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_users_stage_1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_margin="16dp"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/app_users_count"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginRight="8dp"
						android:padding="14dp"
						android:background="#F5F5F5"
						android:orientation="vertical"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/app_users_count_top"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginBottom="4dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/app_users_count_title"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Total Users"
								android:textSize="20sp"
								android:textStyle="bold"
								android:textColor="#000000"
								android:singleLine="true"
								android:layout_weight="1" />
							<ImageView
								android:id="@+id/app_users_count_arrow"
								android:layout_width="18dp"
								android:layout_height="18dp"
								android:src="@drawable/arrow_icms_2"
								android:scaleType="centerInside" />
						</LinearLayout>
						<TextView
							android:id="@+id/app_users_count_num"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="0"
							android:textSize="25sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:singleLine="true" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/app_online_users_count"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="8dp"
						android:padding="14dp"
						android:background="#F5F5F5"
						android:orientation="vertical"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/app_online_users_count_top"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginBottom="4dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/app_online_users_count_title"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Online Users"
								android:textSize="20sp"
								android:textStyle="bold"
								android:textColor="#000000"
								android:singleLine="true"
								android:layout_weight="1" />
							<ImageView
								android:id="@+id/app_online_users_count_arrow"
								android:layout_width="18dp"
								android:layout_height="18dp"
								android:src="@drawable/arrow_icms_2"
								android:scaleType="centerInside" />
						</LinearLayout>
						<TextView
							android:id="@+id/app_online_users_count_num"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="0"
							android:textSize="25sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:singleLine="true" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_users_stage_2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:layout_marginRight="16dp"
					android:layout_marginBottom="16dp"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/app_banned_users_count"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginRight="8dp"
						android:padding="14dp"
						android:background="#F5F5F5"
						android:orientation="vertical"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/app_banned_users_count_top"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginBottom="4dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/app_banned_users_count_title"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Banned Users"
								android:textSize="20sp"
								android:textStyle="bold"
								android:textColor="#000000"
								android:singleLine="true"
								android:layout_weight="1" />
							<ImageView
								android:id="@+id/app_banned_users_count_arrow"
								android:layout_width="18dp"
								android:layout_height="18dp"
								android:src="@drawable/arrow_icms_2"
								android:scaleType="centerInside" />
						</LinearLayout>
						<TextView
							android:id="@+id/app_banned_users_count_num"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="0"
							android:textSize="25sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:singleLine="true" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/app_premium_users_count"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="8dp"
						android:padding="14dp"
						android:background="#F5F5F5"
						android:orientation="vertical"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/app_premium_users_count_top"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_marginBottom="4dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/app_premium_users_count_title"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Premium Users"
								android:textSize="20sp"
								android:textStyle="bold"
								android:textColor="#000000"
								android:singleLine="true"
								android:layout_weight="1" />
							<ImageView
								android:id="@+id/app_premium_users_count_arrow"
								android:layout_width="18dp"
								android:layout_height="18dp"
								android:src="@drawable/arrow_icms_2"
								android:scaleType="centerInside" />
						</LinearLayout>
						<TextView
							android:id="@+id/app_premium_users_count_num"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="0"
							android:textSize="25sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:singleLine="true" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_update_manager"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:layout_marginRight="16dp"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_update_manager_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_update_manager_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="App Update Manager"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_update_manager_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_update_manager_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Submit new update to the application."
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_server_manager"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:layout_marginRight="16dp"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_server_manager_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_server_manager_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Server Manager"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_server_manager_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_server_manager_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Shut down the server, place the application under temporary maintenance, and more."
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_user_manager"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:layout_marginRight="16dp"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_user_manager_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_user_manager_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Manage Users"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_user_manager_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_user_manager_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Verify, block and manage users of the app."
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_logout"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginLeft="16dp"
					android:layout_marginRight="16dp"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_logout_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_logout_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Log out"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#F44336"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_logout_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</ScrollView>
	</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
</LinearLayout>
