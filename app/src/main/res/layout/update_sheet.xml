<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/FatherLayout"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:padding="8dp"
		android:background="#FFFFFF"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear11"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/g_play_store_logo"
				android:focusable="false"
				android:layout_width="100dp"
				android:layout_height="27dp"
				android:src="@drawable/g_play_store_light"
				android:scaleType="fitCenter" />
			<LinearLayout
				android:id="@+id/space"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal"
				android:layout_weight="1" />
			<ImageView
				android:id="@+id/cross_ic"
				android:focusable="false"
				android:layout_width="20dp"
				android:layout_height="30dp"
				android:src="@drawable/baseline_logout_black_48dp"
				android:scaleType="fitCenter" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/container"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="vertical">
			<TextView
				android:id="@+id/update_available_title"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="4dp"
				android:text="Update available"
				android:textSize="25sp"
				android:textStyle="bold"
				android:textColor="#424242" />
			<TextView
				android:id="@+id/dialog_subtitle"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginBottom="15dp"
				android:paddingLeft="4dp"
				android:paddingRight="4dp"
				android:paddingBottom="4dp"
				android:text="To use the app, download the leatest version."
				android:textSize="14sp"
				android:textColor="#9E9E9E" />
			<LinearLayout
				android:id="@+id/app_holder"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="4dp"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear6"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<ImageView
						android:id="@+id/app_icon"
						android:focusable="false"
						android:layout_width="60dp"
						android:layout_height="60dp"
						android:src="@drawable/appicon"
						android:scaleType="fitCenter" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_text_holder"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:orientation="vertical"
					android:layout_weight="1">
					<TextView
						android:id="@+id/app_name"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="4dp"
						android:text="App name"
						android:textSize="20sp"
						android:textStyle="bold"
						android:textColor="#424242"
						android:maxWidth="250dp" />
					<LinearLayout
						android:id="@+id/useless_holder"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:paddingLeft="2dp"
						android:gravity="left|center_vertical"
						android:orientation="horizontal">
						<ImageView
							android:id="@+id/censor_ic"
							android:focusable="false"
							android:layout_width="15dp"
							android:layout_height="15dp"
							android:src="@drawable/everyone_ic"
							android:scaleType="fitCenter" />
						<TextView
							android:id="@+id/ratedfor_txt"
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="4dp"
							android:paddingTop="2dp"
							android:paddingRight="4dp"
							android:paddingBottom="2dp"
							android:text="12+"
							android:textSize="14sp"
							android:textColor="#757575" />
						<ImageView
							android:id="@+id/size_ic"
							android:focusable="false"
							android:layout_width="15dp"
							android:layout_height="15dp"
							android:layout_marginLeft="15dp"
							android:src="@drawable/ic_vertical_align_bottom_black"
							android:scaleType="fitCenter" />
						<TextView
							android:id="@+id/update_size"
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="4dp"
							android:paddingTop="2dp"
							android:paddingRight="4dp"
							android:paddingBottom="2dp"
							android:text="17.9 MB"
							android:textSize="14sp"
							android:textColor="#757575" />
						<ImageView
							android:id="@+id/version_ic"
							android:focusable="false"
							android:layout_width="15dp"
							android:layout_height="15dp"
							android:layout_marginLeft="15dp"
							android:src="@drawable/trending_topic"
							android:scaleType="fitCenter" />
						<TextView
							android:id="@+id/version_txt"
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingLeft="4dp"
							android:paddingTop="2dp"
							android:paddingRight="4dp"
							android:paddingBottom="2dp"
							android:text="Version..."
							android:textSize="14sp"
							android:textColor="#757575" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/DownloadLayout"
				android:layout_width="match_parent"
				android:layout_height="10dp"
				android:padding="2dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<ProgressBar
					android:id="@+id/progressbar1"
					android:focusable="false"
					android:layout_width="match_parent"
					android:layout_height="15dp"
					android:layout_marginLeft="8dp"
					android:layout_marginTop="2dp"
					android:layout_marginRight="8dp"
					android:layout_marginBottom="5dp"
					android:indeterminate="false"
					style="?android:progressBarStyleHorizontal"
					android:progressDrawable="@drawable/progress"
					android:minHeight="15dip"
					android:maxHeight="15dip" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/whatisnew_holder"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="5dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear10"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/useless_layout"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:layout_weight="1">
						<TextView
							android:id="@+id/whats_new_title"
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="What's new"
							android:textSize="18sp"
							android:textStyle="bold"
							android:textColor="#424242" />
						<TextView
							android:id="@+id/update_push_date"
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Updated on..."
							android:textSize="10sp"
							android:textColor="#BDBDBD" />
					</LinearLayout>
					<ImageView
						android:id="@+id/see_more_ic"
						android:focusable="false"
						android:layout_width="15dp"
						android:layout_height="15dp"
						android:src="@drawable/arrow_icms_2"
						android:scaleType="fitCenter"
						android:rotation="90" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/whats_new_layout"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_marginBottom="20dp"
					android:background="#F6F7FB"
					android:orientation="vertical">
					<TextView
						android:id="@+id/whats_new_subtitle"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="7dp"
						android:paddingTop="4dp"
						android:paddingRight="7dp"
						android:paddingBottom="4dp"
						android:background="@android:color/transparent"
						android:text="Initial release\n - Bug fixed.\n - Refreshed UI."
						android:textSize="12sp"
						android:textColor="#757575"
						android:maxHeight="180dp" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/btn_holder"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="3dp"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/more_btn"
					android:layout_width="155dp"
					android:layout_height="42dp"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<TextView
						android:id="@+id/more_txt"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:gravity="center_horizontal|center_vertical"
						android:text="Open in Chrome"
						android:textSize="16sp"
						android:textColor="#445E91"
						android:layout_weight="1" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/divider"
					android:layout_width="1dp"
					android:layout_height="match_parent"
					android:layout_marginLeft="10dp"
					android:layout_marginRight="10dp"
					android:padding="8dp"
					android:background="@android:color/transparent"
					android:orientation="vertical" />
				<LinearLayout
					android:id="@+id/update_btn"
					android:layout_width="155dp"
					android:layout_height="42dp"
					android:background="#445E91"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<TextView
						android:id="@+id/update_txt"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:background="@android:color/transparent"
						android:gravity="center_horizontal|center_vertical"
						android:text="Update"
						android:textSize="16sp"
						android:textStyle="bold"
						android:textColor="#FFFFFF" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
