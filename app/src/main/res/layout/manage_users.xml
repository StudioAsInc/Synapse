<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/body"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/top"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/mBack"
				android:layout_width="48dp"
				android:layout_height="48dp"
				android:padding="12dp"
				android:src="@drawable/ic_arrow_back_black"
				android:scaleType="fitCenter" />
			<TextView
				android:id="@+id/mTitle"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="8dp"
				android:layout_marginRight="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="User Management Panel"
				android:textSize="18sp"
				android:textStyle="bold"
				android:textColor="#000000"
				android:layout_weight="1" />
			<ImageView
				android:id="@+id/spc"
				android:layout_width="48dp"
				android:layout_height="48dp"
				android:padding="12dp"
				android:src="@drawable/default_image"
				android:scaleType="fitCenter"
				android:alpha="0.0" />
		</LinearLayout>
		<ScrollView
			android:id="@+id/scroll"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#FFFFFF"
			android:scrollbars="none">
			<LinearLayout
				android:id="@+id/scroll_body"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="16dp"
				android:orientation="vertical">
				<TextView
					android:id="@+id/primary_tools_title"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginBottom="16dp"
					android:text="Primary Tools"
					android:textSize="25sp"
					android:textStyle="bold"
					android:textColor="#000000" />
				<LinearLayout
					android:id="@+id/search_user_stage"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="horizontal">
					<EditText
						android:id="@+id/search_user_stage_input"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginRight="16dp"
						android:padding="14dp"
						android:background="#F5F5F5"
						android:textSize="16sp"
						android:textColor="#000000"
						android:hint="Enter User UID"
						android:textColorHint="#757575"
						android:singleLine="true"
						android:imeOptions="actionDone"
						android:layout_weight="1" />
					<ImageView
						android:id="@+id/search_user_stage_continue"
						android:layout_width="46dp"
						android:layout_height="46dp"
						android:padding="12dp"
						android:background="#F5F5F5"
						android:src="@drawable/arrow_icms_2"
						android:scaleType="centerInside" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_blocked_users"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="16dp"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_blocked_users_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_blocked_users_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Banned Accounts"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_blocked_users_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_blocked_users_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Manage banned accounts from the app."
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_verified_users"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_verified_users_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_verified_users_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Verified Accounts"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_verified_users_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_verified_users_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Manage verified accounts."
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_verify_requests"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_verify_requests_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_verify_requests_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Verification Requests"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_verify_requests_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_verify_requests_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Manage account verification requests from users."
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_report_requests"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_report_requests_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_report_requests_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Report Request"
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_report_requests_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_report_requests_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Review users reported by other users."
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
				<TextView
					android:id="@+id/tools_for_testing_title"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginBottom="16dp"
					android:text="Tools for Testing"
					android:textSize="25sp"
					android:textStyle="bold"
					android:textColor="#000000" />
				<LinearLayout
					android:id="@+id/app_create_custom_user"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginBottom="16dp"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_create_custom_user_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_create_custom_user_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Create Custom User(under development) "
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_create_custom_user_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_create_custom_user_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Create custom users for your app. (use for testing)"
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/app_send_fake_data"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="14dp"
					android:background="#FFFFFF"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/app_send_fake_data_top"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="4dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/app_send_fake_data_title"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Send Fake Data(under development) "
							android:textSize="20sp"
							android:textStyle="bold"
							android:textColor="#000000"
							android:layout_weight="1" />
						<ImageView
							android:id="@+id/app_send_fake_data_arrow"
							android:layout_width="18dp"
							android:layout_height="18dp"
							android:src="@drawable/arrow_icms_2"
							android:scaleType="centerInside" />
					</LinearLayout>
					<TextView
						android:id="@+id/app_send_fake_data_subtext"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Send users as many fake followers, profile likes and others as you want. (use for testing)"
						android:textSize="18sp"
						android:textColor="#757575" />
				</LinearLayout>
			</LinearLayout>
		</ScrollView>
	</LinearLayout>
</LinearLayout>
